{% extends 'facturepaiement/layout.html.twig' %}

{% block content %}
    <h1>Facturepaiement</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ facturePaiement.id }}</td>
            </tr>
            <tr>
                <th>Montant</th>
                <td>{{ facturePaiement.montant }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('facturepaiement_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('facturepaiement_edit', { 'id': facturePaiement.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
