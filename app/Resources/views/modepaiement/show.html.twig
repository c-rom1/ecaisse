{% extends 'modepaiement/layout.html.twig' %}

{% block content %}
    <h1>Modepaiement</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ modePaiement.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ modePaiement.code }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ modePaiement.libelle }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('modepaiement_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('modepaiement_edit', { 'id': modePaiement.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
